{
  "exclude": [
    "**/bin",
    "**/bower_components",
    "**/jspm_packages",
    "**/node_modules",
    "**/obj",
    "**/platforms"
  ],

  "RabbitMqConfiguration": {
    "Host": "localhost",
    "Port": 5672,
    "Connection": {
      "UserName": "guest",
      "Password": "guest"
    },
    "Exchanges": [
      {
        "Name": "MyTestExchange",
        "Type": "topic"
      },
      {
        "Name": "ErrorExchange",
        "Type": "topic"
      }

    ],
    "Queues": [
      {
        "Name": "MyTestQueue"
      },
      {
        "Name": "EasyNetQ_Default_Error_Queue"
      }
    ],
    "Binding": [
      {
        "ExchangeName": "MyTestExchange",
        "QueueName": "MyTestQueue",
        "RoutingKey": "*",
        "Args": {
          "x-dead-letter-exchange": "ErrorExchange"
        }
      },
      {
        "ExchangeName": "ErrorExchange",
        "QueueName": "EasyNetQ_Default_Error_Queue",
        "RoutingKey": "*",
        "Args": {
          "x-dead-letter-exchange": "ErrorExchange"
        }
      }
    ] 

  } 
}